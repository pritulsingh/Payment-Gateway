{
  "name": "morphpay-sdk",
  "version": "1.0.5",
  "type": "module",
  "description": "Official JavaScript SDK for MorphPay payment processing on Morph blockchain",
  "main": "dist/index.cjs",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "browser": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "npm run clean && npx rollup -c",
    "build:dev": "rollup -c -w",
    "clean": "rimraf dist/",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "dev": "vite",
    "dev:vite": "vite",
    "build:vite": "vite build",
    "preview": "vite preview",
    "start": "vite",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint && npm run test && npm run build",
    "release": "npm run prepublishOnly && npm publish",
    "playground": "cd playground && npm start",
    "demo": "cd demo && python -m http.server 8080",
    "check": "npm run lint && npm run test",
    "dev:app": "vite",
    "build:sdk": "rollup -c",
    "dev:sdk": "rollup -c -w"
  },
  "keywords": [
    "payment",
    "payments",
    "morphpay",
    "morph",
    "blockchain",
    "web3",
    "ethereum",
    "sdk",
    "javascript",
    "react",
    "checkout",
    "ecommerce",
    "crypto",
    "defi"
  ],
  "author": "MorphPay <support@morphpay.com>",
  "license": "MIT",
  "homepage": "https://morphpay.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/morphpay/morphpay-js.git"
  },
  "bugs": {
    "url": "https://github.com/morphpay/morphpay-js/issues"
  },
  "dependencies": {
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "tailwind-merge": "^3.3.1",
    "web3": "^1.10.4"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@eslint/js": "^9.32.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.3.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-react": "^4.7.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.9.6",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^5.9.2",
    "vite": "^7.0.6"
  },
  "optionalDependencies": {
    "canvas": "^2.11.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/src/__tests__/setup.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.test.(js|jsx)",
      "<rootDir>/src/**/*.test.(js|jsx)"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/*.test.{js,jsx}",
      "!src/__tests__/**"
    ],
    "testTimeout": 10000,
    "moduleNameMapping": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/preset-react"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2021": true,
      "node": true,
      "jest": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {
      "no-unused-vars": "warn",
      "no-console": "off",
      "prefer-const": "error",
      "no-var": "error"
    }
  }
}
