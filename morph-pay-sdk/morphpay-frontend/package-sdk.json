{
  "name": "morphpay-sdk",
  "version": "1.0.0",
  "description": "Official JavaScript SDK for MorphPay payment processing",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "browser": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "build:dev": "rollup -c -w",
    "clean": "rimraf dist/",
    "prebuild": "npm run clean",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .js,.jsx",
    "prepublishOnly": "npm run build",
    "release": "npm run build && npm publish"
  },
  "keywords": [
    "payment",
    "payments",
    "morphpay",
    "sdk",
    "javascript",
    "react",
    "checkout",
    "ecommerce"
  ],
  "author": "MorphPay <support@morphpay.com>",
  "license": "MIT",
  "homepage": "https://morphpay.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/morphpay/morphpay-js.git"
  },
  "bugs": {
    "url": "https://github.com/morphpay/morphpay-js/issues"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-react": "^7.23.5",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-json": "^6.0.1",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.9.6",
    "rollup-plugin-dts": "^6.1.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}